        "uat": "npm run icon && cross-env TS_NODE_PROJECT=\"./build/tsconfig-for-webpack-config.json\" DOMAIN='uat' webpack --config='./build/webpack/webpack.prod.ts'",
        "build": "npm run icon && cross-env TS_NODE_PROJECT=\"./build/tsconfig-for-webpack-config.json\" DOMAIN='production' webpack --config='./build/webpack/webpack.prod.ts'"
    },
